{
    "name": "near-care",
    "version": "0.0.1",
    "license": "UNLICENSED",
    "scripts": {
        "build": "npm run build:contract && env-cmd -f ./neardev/dev-account.env next build ./src",
        "build:contract": "node contract/compile.js",
        "build:contract:debug": "node contract/compile.js --debug",
        "build:web": "env-cmd -f ./neardev/dev-account.env next build ./src",
        "dev:web": "env-cmd -f ./neardev/dev-account.env next dev ./src",
        "dev:deploy:contract": "near dev-deploy -f",
        "deploy:contract": "near deploy",
        "deploy": "npm run build && npm run deploy:contract",
        "prestart": "npm run build:contract:debug && npm run dev:deploy:contract",
        "start": "echo The app is starting! It will automatically open in your browser when ready && env-cmd -f ./neardev/dev-account.env npm run dev:web",
        "dev": "nodemon --watch contract/src -e rs --exec \"npm run start\"",
        "test": "npm run build:contract:debug && cd contract && cargo test -- --nocapture && cd .. && jest test --runInBand"
    },
    "devDependencies": {
        "@types/glidejs__glide": "^3.4.1",
        "@types/node": "17.0.21",
        "@types/react": "17.0.39",
        "@types/react-datepicker": "^4.3.4",
        "@types/redux-logger": "^3.0.9",
        "@types/regenerator-runtime": "^0.13.1",
        "autoprefixer": "^10.4.2",
        "babel-preset-env": "^1.7.0",
        "eslint": "8.10.0",
        "eslint-config-next": "12.1.0",
        "gh-pages": "~3.2.3",
        "jest": "~27.5.1",
        "jest-environment-node": "~27.5.1",
        "near-cli": "~3.2.0",
        "nodemon": "~2.0.15",
        "postcss": "^8.4.7",
        "shelljs": "~0.8.5",
        "tailwindcss": "^3.0.23",
        "typescript": "4.6.2"
    },
    "dependencies": {
        "@glidejs/glide": "^3.5.2",
        "@headlessui/react": "^1.5.0",
        "@heroicons/react": "^1.0.5",
        "@reduxjs/toolkit": "^1.8.0",
        "@tailwindcss/aspect-ratio": "^0.4.0",
        "@tailwindcss/forms": "^0.4.0",
        "@tailwindcss/line-clamp": "^0.3.1",
        "@tailwindcss/typography": "^0.5.2",
        "axios": "^0.26.0",
        "classnames": "^2.3.1",
        "env-cmd": "^10.1.0",
        "fs": "^0.0.1-security",
        "ipfs-core": "^0.14.1",
        "near-api-js": "~0.44.2",
        "next": "12.1.0",
        "nft.storage": "^6.0.1",
        "node-sass": "^7.0.1",
        "react": "17.0.2",
        "react-datepicker": "^4.7.0",
        "react-dom": "17.0.2",
        "react-player": "^2.9.0",
        "react-redux": "^7.2.6",
        "react-toastify": "^8.2.0",
        "redux-logger": "^3.0.6",
        "redux-persist": "^6.0.0",
        "regenerator-runtime": "^0.13.9",
        "sass": "^1.49.9"
    },
    "jest": {
        "testEnvironment": "near-cli/test_environment",
        "testPathIgnorePatterns": [
            "<rootDir>/contract/",
            "<rootDir>/node_modules/"
        ]
    }
}